.num {
  font-size: 1.5em;
  color: #9d9d9d;
  text-align: center;
  margin-top: -20rpx;
  margin-bottom: 20rpx;
}

.day {
  display: flex;
  justify-content: center;
  flex-direction: row;
  align-content: center;
}

.text {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 175rpx;
  color: #35a6df;
}

.num1 {
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat;
  background-size: 100% 100%;
  -moz-background-size: 100% 100%;
  width: 100rpx;
  height: 162rpx;
  border-radius: 15rpx 15rpx 15rpx 15rpx;
  margin: 10rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color: white;
  font-size: 130rpx;
}

.cu-bar .content {
  position: absolute;
  text-align: center;
  width: calc(100% - 340rpx);
  left: 0;
  right: 0;
  bottom: 0;
  top: 0;
  margin: auto;
  height: 60rpx;
  font-size: 32rpx;
  line-height: 60rpx;
  cursor: none;
  pointer-events: none;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;

}

.text-fkblue,
.line-fkblue,
.lines-fkblue {
  color: #35a6df;
}

.bg-fkblue {
  background-color: #35a6df;
  color: white;
}

.form {
  width: 600rpx;
  left: 5%;
  border-radius: 30rpx;
}

.input-box {
  width: 85%;
  height: 90rpx;
  background-color: #f1f1f1;
  margin-left: 7.5%;
  border-radius: 50rpx 50rpx 50rpx 50rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  margin-bottom: 30rpx;
  font-size: 1em;
}

.input-title {
  width: 150rpx;
}

.input {
  width: 300rpx;
}